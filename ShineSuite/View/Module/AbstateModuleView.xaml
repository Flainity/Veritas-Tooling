<UserControl x:Class="ShineSuite.View.Module.AbstateModuleView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:ShineSuite.View.Module"
             xmlns:modules="clr-namespace:ShineSuite.Module.Abstate.View;assembly=ShineSuite.Module.Abstate"
             mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="700">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="250"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        
        <modules:ListView/>
        <modules:GeneralView Grid.Column="1"/>
    </Grid>
</UserControl>
